<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <OutputType>Library</OutputType>
        <Nullable>Enable</Nullable>
        <LangVersion>Latest</LangVersion>
        <RootNamespace>RRON</RootNamespace>
        <AssemblyName>RRON</AssemblyName>
        <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <Company>Rubiksmaster02</Company>
        <NeutralLanguage>en-US</NeutralLanguage>
        <PackageVersion>1.0.2</PackageVersion>
        <Title>RRON</Title>
        <Authors>Rubiksmaster02</Authors>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Copyright>MIT</Copyright>
        <RepositoryUrl>https://github.com/Redageddon/RRON</RepositoryUrl>
        <NoWarn>SA1009</NoWarn>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <PlatformTarget>x64</PlatformTarget>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Collections.Pooled" Version="1.0.82" />
        <PackageReference Include="FastMember" Version="1.5.0" />
        <PackageReference Include="JetBrains.Profiler.Api" Version="1.1.7" />
        <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="3.1.7" />
        <PackageReference Include="System.Memory" Version="4.5.4" />
    </ItemGroup>

</Project>
